{
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "breadcrumbs.enabled": false,
  "diffEditor.ignoreTrimWhitespace": true,
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorSurroundingLines": 12,
  "editor.fontFamily": "FiraCode Nerd Font",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.formatOnSave": true,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.quickSuggestions": {
    "comments": "inline",
    "other": "inline",
    "strings": "inline"
  },
  "editor.renderLineHighlight": "gutter",
  "editor.renderWhitespace": "trailing",
  "editor.rulers": [
    120,
    80
  ],
  "editor.semanticHighlighting.enabled": true,
  "editor.tabSize": 2,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmPasteNative": false,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    ".env": ".env.*",
    "*.cts": "${capture}.*.ts",
    "*.js": "${capture}.js.map, ${capture}.*.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js, ${capture}.*.jsx",
    "*.mts": "${capture}.*.ts",
    "*.ts": "${capture}.js, ${capture}.*.ts",
    "*.tsx": "${capture}.ts, ${capture}.*.ts, ${capture}.*.tsx",
    "Dockerfile": ".dockerignore",
    "next.config.*s": "next-env.d.ts, postcss.config.*js",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lock",
    "prettier.config.*js": ".prettierignore",
    "tailwind.config.*s": "postcss.config.*s",
    "tsconfig.json": "tsconfig.*.json"
  },
  "explorer.incrementalNaming": "smart",
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    ".sequelizerc": "javascript",
    "*.css": "tailwindcss",
    "*.h": "c",
    "**/requirements{/**,*}.{txt,in}": "pip-requirements",
    "complex": "c"
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "git.autofetch": true,
  "git.blame.editorDecoration.enabled": true,
  "git.closeDiffOnOperation": true,
  "git.confirmSync": false,
  "git.mergeEditor": true,
  "git.openRepositoryInParentFolders": "always",
  "git.replaceTagsWhenPull": true,
  "git.suggestSmartCommit": false,
  "git.terminalGitEditor": true,
  "git.untrackedChanges": "separate",
  "github.gitProtocol": "ssh",
  "html.format.preserveNewLines": false,
  "html.format.templating": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "json.schemaDownload.enable": true,
  "material-icon-theme.folders.theme": "specific",
  "playwright.reuseBrowser": false,
  "scm.defaultViewMode": "tree",
  "search.defaultViewMode": "tree",
  "search.smartCase": true,
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorStyleInactive": "line",
  "terminal.integrated.defaultProfile.linux": "fish",
  "terminal.integrated.fontLigatures.enabled": true,
  "terminal.integrated.hideOnStartup": "whenEmpty",
  "terminal.integrated.showExitAlert": false,
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.tabs.hideCondition": "never",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.commandCenter": false,
  "window.confirmSaveUntitledWorkspace": false,
  "window.titleBarStyle": "custom",
  "workbench.editor.customLabels.patterns": {
    "**/app/**/layout.tsx": "${dirname} - Layout",
    "**/app/**/page.tsx": "${dirname} - Page",
    "**/components/**/index.tsx": "${dirname} - Component"
  },
  "workbench.editor.limit.enabled": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.layoutControl.enabled": false,
  "workbench.navigationControl.enabled": false,
  "workbench.startupEditor": "none",
  "biome.lsp.trace.server": "messages",
  "biome.requireConfiguration": true,
  "workbench.colorTheme": "Adwaita Dark"
}
